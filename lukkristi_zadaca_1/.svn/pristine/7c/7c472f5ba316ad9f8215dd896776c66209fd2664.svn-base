
package org.foi.nwtis.lukkristi.zadaca_1;

import java.io.Serializable;

/**
 *
 * @author lukkristi
 */
public class Avion implements Serializable{
    
    private String naziv;
    private Aerodrom aerodrom;
    private String vrijemePolijetanja;
    private String vrijemeSlijetanja;

    public Avion(String naziv, Aerodrom aerodrom, String vrijemePolijetanja) {
        this.naziv = naziv;
        this.aerodrom = aerodrom;
        this.vrijemePolijetanja = vrijemePolijetanja;
    }

    /**
     * vraća naziv aviona
     * @return
     */
    public String getNaziv() {
        return naziv;
    }

    /**
     * vraća naziv aerodroma gdje se nalazi avion
     * @return
     */
    public Aerodrom getAerodrom() {
        return aerodrom;
    }

    /**
     * Vraća vrijeme polijetanja aviona
     * @return
     */
    public String getVrijemePolijetanja() {
        return vrijemePolijetanja;
    }

    /**
     * Vraća vrijeme slijetanja aviona
     * @return
     */
    public String getVrijemeSlijetanja() {
        return vrijemeSlijetanja;
    }

    /**
     * Postavlja naziv aviona
     * @param naziv
     */
    public void setNaziv(String naziv) {
        this.naziv = naziv;
    }

    /**
     * Postavlja naziv aerodroma aviona
     * @param aerodrom
     */
    public void setAerodrom(Aerodrom aerodrom) {
        this.aerodrom = aerodrom;
    }

    /**
     * Postavlja vrijeme polijetanja aviona
     * @param vrijemePolijetanja
     */
    public void setVrijemePolijetanja(String vrijemePolijetanja) {
        this.vrijemePolijetanja = vrijemePolijetanja;
    }

    /**
     * Postavlja vrijeme slijetanja aviona
     * @param vrijemeSlijetanja
     */
    public void setVrijemeSlijetanja(String vrijemeSlijetanja) {
        this.vrijemeSlijetanja = vrijemeSlijetanja;
    }
    
}
